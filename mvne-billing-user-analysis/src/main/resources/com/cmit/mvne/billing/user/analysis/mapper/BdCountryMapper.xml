<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cmit.mvne.billing.user.analysis.mapper.BdCountryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cmit.mvne.billing.user.analysis.entity.BdCountry">
        <result column="ISO" property="iso" />
        <result column="COUNTRY_NAME" property="countryName" />
        <result column="COUNTRY_DESCRIPTION" property="countryDescription" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ISO, COUNTRY_NAME, COUNTRY_DESCRIPTION
    </sql>

</mapper>
