<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cmit.mvne.billing.user.analysis.mapper.SysCountryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cmit.mvne.billing.user.analysis.entity.SysCountry">
        <result column="COUNTRY_ID" property="countryId" />
        <result column="NAME" property="name" />
        <result column="NAME_ABBREVIATION" property="nameAbbreviation" />
        <result column="COUNTRY_CODE" property="countryCode" />
        <result column="GEOGRAPHIC_AREA_ID" property="geographicAreaId" />
        <result column="TYPE" property="type" />
        <result column="LANGUAGE_ID" property="languageId" />
        <result column="TIME_ZONE_ID" property="timeZoneId" />
        <result column="TIME_ZONE" property="timeZone" />
        <result column="TENANT_ID" property="tenantId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        COUNTRY_ID, NAME, NAME_ABBREVIATION, COUNTRY_CODE, GEOGRAPHIC_AREA_ID, TYPE, LANGUAGE_ID, TIME_ZONE_ID, TIME_ZONE, TENANT_ID
    </sql>

</mapper>
